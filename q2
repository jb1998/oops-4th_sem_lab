#include <iostream>

using namespace std;
int check(int year){
  if(year%400==0)
    return 1;
  if(year%100==0)
    return 0;
  if(year%4==0){
    return 1;
  }
  return 0;
}
int main()
{

    int sum=0;
cout<<"1 represent leap year and 0 represent not a leap year"<<endl;

    cout<<"enter year"<<endl;

        cin>>sum;
    cout<<check(sum)<<endl;
       return 0;

    }
